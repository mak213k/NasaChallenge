   "source": [
    "def compose_ai(value):\n",
    "    name = ' Apollo '.join(value.split()[::-1])\n",
    "    return name[-10::]\n",
    "\n",
    "nakamura_1983_ai_locations['label'] = nakamura_1983_ai_locations['AI'].apply(compose_ai)\n",
    "\n",
    "nakamura_1983_ai_locations"
   ]
